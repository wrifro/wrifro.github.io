    "\n",
    "n = 100\n",
    "x = np.random.rand(n, 1)\n",
    "y = w1*x + w0 + 0.1*np.random.randn(n, 1)\n",
    "\n",
    "plt.scatter(x, y)\n",
    "labels = plt.gca().set(xlabel = \"Feature (x)\", ylabel = \"Target (y)\")"
   ]
